{
  "comment": "Configuration for implementing Java Enterprise best practices with Spring Boot",
  "tech_stack": "java-spring",
  "problem_commands": {
    "type_check": ["./gradlew", "compileJava"],
    "lint": ["./gradlew", "spotlessApply"]
  },
  "best_practices_files": ["./best_practices_guides/JAVA_ENTERPRISE_BEST_PRACTICES.md"],
  "chunk_size": 20,
  "agents": 12,
  "session": "claude_java_best_practices",
  "stagger": 18.0,
  "wait_after_cc": 25.0,
  "check_interval": 20,
  "skip_regenerate": false,
  "skip_commit": false,
  "auto_restart": true,
  "no_monitor": false,
  "attach": false,
  "prompt_file": "prompts/default_best_practices_prompt_java.txt",
  "context_threshold": 25,
  "idle_timeout": 150,
  "max_errors": 5,
  "git_branch": "main",
  "git_remote": "origin",
  "tmux_kill_on_exit": true,
  "tmux_mouse": true,
  "fast_start": false,
  "full_backup": false,
  "max_agents": 25
} 