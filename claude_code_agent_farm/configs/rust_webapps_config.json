{
  "comment": "Configuration for implementing Rust web applications best practices",
  "tech_stack": "rust-web",
  "problem_commands": {
    "type_check": ["cargo", "check", "--all-targets"],
    "lint": ["cargo", "clippy", "--all-targets", "--all-features", "--", "-D", "warnings"]
  },
  "best_practices_files": ["./best_practices_guides/RUST_WEBAPPS_BEST_PRACTICES.md"],
  "chunk_size": 25,
  "agents": 10,
  "session": "claude_rust_web_best_practices",
  "stagger": 20.0,
  "wait_after_cc": 30.0,
  "check_interval": 20,
  "skip_regenerate": false,
  "skip_commit": false,
  "auto_restart": true,
  "no_monitor": false,
  "attach": false,
  "prompt_file": "prompts/default_best_practices_prompt_rust_web.txt",
  "context_threshold": 20,
  "idle_timeout": 150,
  "max_errors": 4,
  "git_branch": "main",
  "git_remote": "origin",
  "tmux_kill_on_exit": true,
  "tmux_mouse": true,
  "fast_start": false,
  "full_backup": false,
  "max_agents": 20
} 