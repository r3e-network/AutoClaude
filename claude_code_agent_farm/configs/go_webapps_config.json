{
  "comment": "Configuration for implementing Go web applications best practices",
  "tech_stack": "go",
  "problem_commands": {
    "type_check": ["go", "build", "./..."],
    "lint": ["golangci-lint", "run", "--fix"]
  },
  "best_practices_files": ["./best_practices_guides/GO_WEBAPPS_BEST_PRACTICES.md"],
  "chunk_size": 25,
  "agents": 12,
  "session": "claude_go_best_practices",
  "stagger": 15.0,
  "wait_after_cc": 20.0,
  "check_interval": 15,
  "skip_regenerate": false,
  "skip_commit": false,
  "auto_restart": true,
  "no_monitor": false,
  "attach": false,
  "prompt_file": "prompts/default_best_practices_prompt_go.txt",
  "context_threshold": 20,
  "idle_timeout": 100,
  "max_errors": 4,
  "git_branch": "main",
  "git_remote": "origin",
  "tmux_kill_on_exit": true,
  "tmux_mouse": true,
  "fast_start": false,
  "full_backup": false,
  "max_agents": 25
} 